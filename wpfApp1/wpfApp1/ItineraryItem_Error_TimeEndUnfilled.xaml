<Window x:Class="trvlApp.ItineraryItem_Error_TimeEndUnfilled"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:trvlApp"
        mc:Ignorable="d"
        Title="ItineraryItem_Error" Height="150" Width="200">
    <Grid>
        <TextBlock TextWrapping="WrapWithOverflow" HorizontalAlignment="Center" Margin="0,15,0,0" VerticalAlignment="Top" Height="78" Width="180">
            Error! 'Time End' must not be empty! 'Time End' should be in the format hours:minutes in a 12 hour format.
        </TextBlock>
        <CheckBox Content="CheckBox" HorizontalAlignment="Left" Margin="404,7,0,0" VerticalAlignment="Top"/>
        <Button Content="OK" HorizontalAlignment="Center" Margin="0,104,0,0" VerticalAlignment="Top" Width="90"/>
    </Grid>
</Window>
