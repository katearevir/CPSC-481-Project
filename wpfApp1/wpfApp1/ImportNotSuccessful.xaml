<Window x:Class="trvlApp.ImportNotSuccessful"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:trvlApp"
        mc:Ignorable="d"
        Title="Import Failed!" Height="100" Width="200">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="8,4,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="29" Width="188" FontFamily="Global User Interface" Text="The code does not exist, please double check and try again!"/>
        <Button Content="Try Again" HorizontalAlignment="Center" Margin="0,51,0,0" VerticalAlignment="Top" FontFamily="Global User Interface" />
        <TextBlock HorizontalAlignment="Left" Margin="8,35,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="9" Width="180" FontFamily="Global User Interface" Text="Reminder: share codes expire after 15 minutes" FontSize="8"/>

    </Grid>
</Window>
